{
	"name": "rental-manager",
	"title": "Rental Manager",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "sh -ac '. ./.env;cp src/lib/backend-$PUBLIC_BACKEND_TYPE.ts src/lib/backend.ts; vite dev --open . --host 0.0.0.0'",
		"build": "sh -ac '. ./.env;cp src/lib/backend-$PUBLIC_BACKEND_TYPE.ts src/lib/backend.ts; vite build'",
		"preview": "sh -ac '. ./.env;cp src/lib/backend-$PUBLIC_BACKEND_TYPE.ts src/lib/backend.ts; vite preview'",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.5",
		"@sveltejs/kit": "^2.6.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.7",
		"@types/node": "^22.5.5",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.47",
		"shadcn-svelte": "^0.13.0",
		"svelte": "^5.0.0-next.260",
		"svelte-add": "^2.7.3",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@capacitor/android": "^6.1.2",
		"@capacitor/cli": "^6.1.2",
		"@capacitor/core": "^6.1.2",
		"@capacitor/ios": "^6.1.2",
		"@inlang/paraglide-js": "^1.11.2",
		"@internationalized/date": "^3.5.5",
		"@supabase/supabase-js": "^2.45.4",
		"bits-ui": "^0.21.15",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.18",
		"formsnap": "^1.0.1",
		"lucide-svelte": "^0.445.0",
		"mode-watcher": "^0.4.1",
		"paneforge": "^0.0.6",
		"pocketbase": "^0.21.5",
		"svelte-sonner": "^0.3.28",
		"sveltekit-i18n": "^2.4.2",
		"sveltekit-superforms": "^2.19.0",
		"tailwind-merge": "^2.5.2",
		"tailwind-variants": "^0.2.1",
		"vaul-svelte": "^0.3.2",
		"zod": "^3.23.8"
	}
}
